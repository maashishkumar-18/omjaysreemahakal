{
  "expo": {
    "name": "om-jay-sree-mahakal",
    "slug": "om-jay-sree-mahakal",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/final_icon.png",
    "scheme": "omjaysreemahakal",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/final_icon.png",
        "backgroundImage": "./assets/images/final_icon.png",
        "monochromeImage": "./assets/images/final_icon.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.maashishkumar18.omjaysreemahakal"
    },
    "extra": {
      "API_BASE_URL": "https://omjaysreemahakal-backend.onrender.com/api",
      "router": {},
      "eas": {
        "projectId": "bbd8e91f-9d56-4ac5-8dc7-598e4798913f"
      }
    },
    "web": {
       "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/final_icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/final_icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
